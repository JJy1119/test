{
  "groups": [
    {
      "name": "mozart.commons.macro",
      "type": "cn.freemud.framework"
    },
    {
       "name": "mozart.commons.network",
       "type": "com.gznb.mozart.commons.configuration.NetworkConfig"
    },
    {
      "name": "mozart.commons.memorycache",
      "type": "com.gznb.mozart.commons.caching.memory.MemoryCacheOptions"
    }
  ],

  "properties": [
    {
      "name": "mozart.commons.macro.app_group",
      "type": "java.lang.String",
      "description": "项目组CODE."
      /*"defaultValue": "UTF-8"*/
    },
    {
      "name": "mozart.commons.macro.app_name",
      "type": "java.lang.String",
      "description": "应用CODE"
    },
    {
      "name": "mozart.commons.macro.version",
      "type": "java.lang.String",
      "description": "应用版本号"
    },
    {
      "name": "mozart.commons.macro.app_display_name",
      "type": "java.lang.String",
      "description": "应用中文名"
    },
    {
      "name": "mozart.commons.network.config",
      "type": "java.util.List<com.gznb.mozart.commons.configuration.NetworkConfigItem>",
      "description": "network配置数组."
    },
    {
      "name": "mozart.commons.network.config.ips",
      "type": "java.util.List<java.lang.String>",
