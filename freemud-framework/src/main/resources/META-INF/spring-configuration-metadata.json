    {
      "name": "mozart.commons.memorycache.scan_frequency",
      "type": "java.lang.Integer",
      "description": "缓存过期的扫描频率(毫秒）"
    },
    {
      "name": "mozart.commons.memorycache.compact",
      "type": "java.lang.Boolean",
      "description": "是否在内存紧张时进行内存收缩（强制过期一部分缓存）"
    }
  ],

  "hints": [
  ]
}